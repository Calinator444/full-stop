{
   "singleAttributePerLine": true,
   "tabWidth": 3,
   "useTabs": false,
   "singleQuote": true,
   "semi": true,
   "bracketSpacing": true,
   "arrowParens": "always",
   "trailingComma": "es5",
   "bracketSameLine": false,
   "printWidth": 80,
   "overrides": [
      {
         "files": "*.html",
         "options": {
            "parser": "angular"
         }
      }
   ]
}
